#
# GCP full-stack deployment ignore file
#

# Ignore everything by default
*

# Include dist folder (client + server bundles)
!/dist/
!/dist/client/
!/dist/client/**
!/dist/server.js
!/dist/**/*.js

# Include server entry points
!/server.ts
!/server.js

# Include deployment metadata
!/app.yaml
!/package.json
!/.gcloudignore

# include runtime configs
!/tsconfig.json
!/tsconfig.*.json
!/tailwind.config.js
!/postcss.config.js
!/vite.config.ts

# Exclude sensitive/dev artifacts
.gcloud-key.json
.env
.env.*
.dev.vars*
node_modules/
src/
worker/
.git/
.vscode/
.idea/
.eslintcache
coverage/
.nyc_output/
*.ts
*.tsx
*.jsx
*.map
*.zip
*.tar.gz
*.log
.DS_Store


